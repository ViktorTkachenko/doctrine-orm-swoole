{
    "name": "opsway/doctrine-orm-swoole",
    "description": "Doctrine EntityManager Decorator for Swoole &amp; coroutine (concurrency requests) support",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Vronskiy",
            "email": "alvro@opsway.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^4.0",
        "doctrine/orm": "^3.0",
        "psr/container": "^1.0 || ^2.0"
    },
    "require-dev": {
        "opsway/psr12-strict-coding-standard": "^1.1",
        "lctrs/psalm-psr-container-plugin": "^1.10",
        "vimeo/psalm": "^5.23"
    },
    "autoload": {
        "psr-4": {
            "OpsWay\\Doctrine\\ORM\\Swoole\\": "src/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
